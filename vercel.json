{
  "version": 2,
  "buildCommand": "next build && npx prisma generate",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NEXTAUTH_URL": {
      "required": true
    },
    "NEXTAUTH_SECRET": {
      "required": true
    },
    "DATABASE_URL": {
      "required": true
    }
  },
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
